{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/remix"
      }
    ],
    "routes": {
      "/assets/(.*)": {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      "/(.*)": {
        "src": "/$1",
        "dest": "/index.html"
      }
    }
  }
  